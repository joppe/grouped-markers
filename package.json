    },
    "keywords": [
        "js",
        "babeljs",
        "systemjs",
        "Google maps"
    ],
    "author": "Joppe Aarts",
    "license": "MIT",
    "devDependencies": {
        "babel": "^5.5.8",
        "babel-core": "^5.1.13",
        "es6-module-loader": "^0.16.5",
        "karma": "^0.12.31",
        "karma-jasmine": "^0.3.5",
        "karma-phantomjs-launcher": "~0.1",
        "karma-systemjs": "^0.3.0",
        "phantomjs-polyfill": "0.0.1",
        "systemjs": "^0.16.7",
        "systemjs-builder": "^0.14.10",
        "react": "^0.14.3",
        "react-dom": "^0.14.3"
    }
